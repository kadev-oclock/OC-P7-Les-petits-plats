{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": ["error"],
      "indent": ["error", 2]
    }
  }
  